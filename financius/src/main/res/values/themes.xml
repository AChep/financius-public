<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="spbStyle">@style/ProgressBar</item>
    </style>

    <style name="ProgressBar" parent="SmoothProgressBar">
        <item name="spb_stroke_separator_length">10dp</item>
        <item name="spb_sections_count">5</item>
        <item name="spb_speed">1.0</item>
        <item name="spb_progressiveStart_speed">1.0</item>
        <item name="spb_progressiveStop_speed">1.0</item>
        <item name="spb_interpolator">spb_interpolator_acceleratedecelerate</item>
        <item name="spb_mirror_mode">false</item>
        <item name="spb_reversed">false</item>
        <item name="spb_colors">@array/progress_bar_colors</item>
        <item name="spb_progressiveStart_activated">false</item>
    </style>

</resources>